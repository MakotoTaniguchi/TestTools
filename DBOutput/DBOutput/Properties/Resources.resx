  <data name="rcQueryPlan" xml:space="preserve">
    <value>SELECT 
    [creation_time]             AS [コンパイル時刻], 
    [last_execution_time]       AS [前回の実行時刻], 
    [execution_count]           AS [実行された回数], 
    [total_worker_time] / 1000  AS [CPU 時間の合計(msec)],
    [total_worker_time] / [execution_count] / 1000 
                                AS [CPU 時間の平均(msec)], 
    [last_worker_time] / 1000   AS [前回実行時のCPU時間(msec)],
    [max_worker_time] / 1000    AS [最大CPU時間(msec)],
    [min_worker_time] / 1000    AS [最小CPU時間(msec)],
    --[name] AS [オブジェクト名],
    SUBSTRING(st.[text], (qs.[statement_start_offset] / 2) + 1,
    ((CASE [statement_end_offset]
        WHEN -1 THEN DATALENGTH(st.[text])
        ELSE qs.[statement_end_offset] END 
            - qs.[statement_start_offset]) / 2) + 1) as [ステートメント],
    sp.[query_plan] 				AS [実行プラン]
FROM sys.dm_exec_query_stats as qs
CROSS APPLY sys.dm_exec_sql_text(qs.[sql_handle]) as st
LEFT OUTER JOIN sys.objects AS so ON so.[object_id] = st.[objectid]
CROSS APPLY sys.dm_exec_query_plan(qs.[plan_handle]) as sp
LEFT OUTER JOIN sys.objects ON so.[object_id] = sp.[objectid]
ORDER BY [total_worker_time] / [execution_count] DESC;</value>
    <comment>クエリプラン情報</comment>
  </data>
